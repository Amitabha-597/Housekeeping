.serchContent{
padding:0 20rpx
}
#goldshop_list {
  width: 100%;
}

#goldshop_list .goldshop_item {
  position: relative;

}

#goldshop_list .goldshop_item .goldl {
  width: 30rpx;
  height: 38rpx;
  position: absolute;
  top: 0rpx;
  right: 30rpx;

}

#goldshop_list .goldshop_item {
  width: 100%;
  margin-top: 20rpx;
  box-shadow: 0rpx 5rpx 27rpx 0rpx rgba(0, 0, 0, 0.11);
  border-radius: 10rpx;
  padding-bottom: 20rpx;
}

#goldshop_list .goldshop_item image {
  width: 100%;
}

#goldshop_list .goldshop_item .shop_msg {
  width: 100%;
  display: flex;
  justify-content: space-between;
}

#goldshop_list .goldshop_item .shop_msg .shop_msg_text {
  width: 80%;
}

#goldshop_list .goldshop_item .shop_name {
  font-size: 30rpx;
  font-weight: bold;
  padding: 5rpx 10rpx;
}

#goldshop_list .goldshop_item .shop_volume .heart {
  width: 24rpx;
  margin-left: 10rpx;
  height: 20rpx;
  vertical-align: middle;
}

#goldshop_list .goldshop_item .shop_volume text {
  padding: 5rpx 10rpx;
  font-size: 24rpx;
  color: #9a9a9a;
}

#goldshop_list .goldshop_item .shop_msg .shop_goshop {
  width: 20%;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 18rpx;
}

#goldshop_list .goldshop_item .shop_msg .shop_goshop button {
  width: 70%;
  height: 50%;
  padding: 0;
  margin: 0;
  font-weight: none;
  background: linear-gradient(0deg, rgba(236, 132, 1, 1), rgba(236, 172, 1, 1));
  border-radius: 2rpx;
  color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
}
#waiter {
  width: 100%;
}

#waiter .waiter_item {
  width: 100%;
  display: flex;
  justify-content: center;
  padding-bottom: 20rpx;
}

#waiter .waiter_item .waiter_img {
  width: 35%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}

#waiter .waiter_item .waiter_img image {
  width: 98%;
  height: 98%;
}

#waiter .waiter_item .waiter_msg {
  height: 100%;
  width: 65%;
  padding-left: 10rpx;
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  justify-content: center;
}

#waiter .waiter_item .waiter_msg view {
  padding: 3rpx 0;
}

#waiter .waiter_item .waiter_msg .price {
  color: #ec8401;
  font-size: 25rpx;
  padding-left: 50rpx;
}

#waiter .waiter_item .waiter_msg .msg_info {
  font-size: 30rpx;
  font-weight: bold;
}

#waiter .waiter_item .waiter_msg .msg_info text {
  margin: 0 10rpx;

}

#waiter .waiter_item .waiter_msg .msg_skill {
  display: flex;
  flex-wrap: wrap;
  font-size: 24rpx;
  color: #999;
}

#waiter .waiter_item .waiter_msg .msg_badge text {
  font-size: 20rpx;
  color: #ec8401;
  margin-right: 10rpx;
  padding: 3rpx 10rpx;
  background: rgba(253, 249, 243, 1);
  border: 1rpx solid rgba(236, 132, 1, 1);
  border-radius: 6rpx;
}

#waiter .waiter_item .waiter_msg .msg_checked {
  width: 100%;
  height: 36%;
  display: flex;
  margin-top: 20rpx;
  justify-content: start;
  align-items: center;
}

#waiter .waiter_item .waiter_msg .msg_checked button {
  background: #ec8401;
  color: #fff;
  width: 165rpx;
  height: 54rpx;
  margin: 20rpx 0 0 0;
  padding: 0 0;
  line-height: 54rpx;
  font-size: 30rpx;
}
.serviceItems {
  width: 689rpx;
  height: 100%;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}

.serviceItemschild {
  width: 328rpx;
  height: 570rpx;
  background: rgba(255, 255, 255, 1);
  border: 2px solid rgba(234, 234, 234, 1);
  border-radius: 12rpx;
  margin-bottom: 30rpx;
}

.serviceItemschild_img {
  width: 328rpx;
  height: 328rpx;
  border-radius: 12rpx;
}

.serviceItemschild_msg {
  width: 100%;
  box-sizing: border-box;
  padding: 0 17rpx;

}

.serviceItemschild_msg_con {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: rgba(34, 34, 34, 1);
}

.serviceItemschild_msg_eva {
  padding: 5rpx 0 rpx;
  font-size: 20rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: rgba(154, 154, 154, 1);
  line-height: 41rpx;

}

.serviceItemschild_msg_eva text {
  margin-right: 20rpx;
}

.serviceItemschild_msg_pric {
  height: 100rpx;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: rgba(236, 132, 1, 1);
  line-height: 41rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.serviceItemschild_msg_btn {
  width: 136rpx;
  height: 54rpx;
  border: 1rpx solid #ECAC01;
  background: rgba(255, 255, 255, 1);
  border-radius: 6rpx;
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: bold;
  display: flex;
  align-items: center;
  justify-content: center;
}

.seds_comment {
  width: 689rpx;
  height: 100%;
}

.seds_comment_title {
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
}






