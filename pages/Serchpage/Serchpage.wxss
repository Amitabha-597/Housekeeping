.Serch{
  height: 100%;
  width: 100%;
}

.header{
  height:100rpx;
  display: flex;
  justify-content: space-around;
  padding-left:20rpx;
  padding-right:20rpx;
  align-items: center;
  flex-direction: row;
  background: #fff;
}
.search{
  width:85%;
  height:70rpx;
  background: rgb(245, 245, 245);
  border-radius:35rpx;
  padding-left: 20rpx;
  display: flex;
  align-items: center;
}
.search>input{
  width: 80%;
  margin-left: 20rpx;
  
}
.cancel{
  color: #999;
}
