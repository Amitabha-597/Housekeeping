.serchHistorytitle{
  margin: 0 auto;
  width: 90%;
  display: flex;
  justify-content: space-between;
}
.serchHistorytitle>text:first-child{
  font-size: 24rpx;
  color: #666;

}
.serchHistorytitle>text:last-child{
  font-size: 22rpx;
  color: #999;

}
.serchHistorymain{
  display: flex;
  align-items: center;
flex-wrap:wrap;
  width: 90%;
  margin:20rpx auto ;
}
.serchHistorymain>text{
  font-size: 30rpx;
  line-height: 30rpx;
  color: #666;
  background: #e2dfdf;
  border-radius:30rpx ;
  padding:8rpx 14rpx ;
  margin:9rpx ;
}
.gold {
  padding: 20rpx 0;
  height: 34rpx;
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.gold .shop {
  font-size: 28rpx;
}

/* .gold .more {
  font-size: 26rpx;
